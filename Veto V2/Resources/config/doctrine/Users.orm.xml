<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VetsBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="integer" column="telephone" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rue" type="string" column="rue" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gouvernorat" type="string" column="gouvernorat" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codepostal" type="integer" column="codePostal" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="3000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="role" type="string" column="role" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
